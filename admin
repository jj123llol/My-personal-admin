			if cmd == "kidnap" then
				local Who = string.sub(msg,space+1)
				local target = GetPlayer(Who)
				tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(300, Enum.EasingStyle.Linear)

				tween = tweenService:Create(target.Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(10000, 0, 5)})
				tween:Play()
				wait(20)
				target.Character.Humanoid.Health = 0
			end



			if cmd == "holy" then
				local Who = string.sub(msg,space+1)
				local target = GetPlayer(Who)
				tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(300, Enum.EasingStyle.Linear)

				tween = tweenService:Create(target.Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(0, 9000, 0)})
				tween:Play()
				wait(10)
				target.Character.Humanoid.Health = 0
			end

			if cmd =="check" then
				wait(0.5)
				local args = {
					[1] = "‚≠êJude/rev was here‚≠ê",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end
			
			
			
			if cmd =="anyone?" then
				wait(0.5)
				local args = {
					[1] = "‚≠êJude/rev was here‚≠ê",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end
			
			
			
			

			if cmd =="laugh" then
				game.Players:Chat("/e laugh")
			end




			if cmd =="wave" then
				game.Players:Chat("/e wave")
			end


			if cmd =="dance3" then
				game.Players:Chat("/e dance3")
			end
			
			
			if cmd =="cheer" then
				game.Players:Chat("/e cheer")
			end

			if cmd =="point" then
				game.Players:Chat("/e point")
			end



			if cmd =="dance2" then
				game.Players:Chat("/e dance2")
			end


			if cmd =="dance1" then
				game.Players:Chat("/e dance1")
			end


			if cmd =="dance" then
				game.Players:Chat("/e dance")
			end

			if cmd =="jump" then
				local Who = string.sub(msg,space+1)
				local target = GetPlayer(Who)
				target.Character.Humanoid.Jump = true
			end
			
			
			

			
			 if cmd =="bang" then
				local Who = string.sub(msg,space+1)
				local target = GetPlayer(Who)
				local ME = game:GetService("Workspace")[x.Name].Torso.CFrame
				target.Character.HumanoidRootPart.CFrame = ME
				wait(0.3)
				local Human = target.Character.Humanoid
				if not Human then
					return
				end
				Human.Sit = true
				task.wait(.1)
				Human.RootPart.CFrame = Human.RootPart.CFrame * CFrame.Angles(math.pi * .15, 0, 0)
				for _, v in ipairs(Human:GetPlayingAnimationTracks()) do
					v:Stop()
					local Human = target.Character.Humanoid
					if not Human then
						return
					end
				end
				wait()
				Human.Sit = true
				task.wait(.1)
				Human.RootPart.CFrame = Human.RootPart.CFrame * CFrame.Angles(math.pi * .5, 0, 0)
				for _, v in ipairs(Human:GetPlayingAnimationTracks()) do
					v:Stop()
					local Human = target.Character.Humanoid
					if not Human then
						return
					end
				end
				wait()
				Human.Sit = true
				task.wait(.1)
				Human.RootPart.CFrame = Human.RootPart.CFrame * CFrame.Angles(math.pi * .5, 0, 0)
				for _, v in ipairs(Human:GetPlayingAnimationTracks()) do
					v:Stop()
					local Human = target.Character.Humanoid
					if not Human then
						return
					end
				end
				wait()
				Human.Sit = true
				task.wait(.1)
				Human.RootPart.CFrame = Human.RootPart.CFrame * CFrame.Angles(math.pi * .5, 0, 0)
				for _, v in ipairs(Human:GetPlayingAnimationTracks()) do
					v:Stop()
				end
			end
			
			
			
			
			
			if cmd == "sit,freeze" then
				local Who = string.sub(msg,space+1)
				local target = GetPlayer(Who)
				local Human = target.Character.Humanoid
				if not Human then
					return
				end
				Human.Sit = true
				
				
				
				wait(0.5)
				target.Character.HumanoidRootPart.Anchored = true
			end
			
			
			
			
			
			
			
			
			
			
			
			
			
			if cmd =="lay" then
				local Who = string.sub(msg,space+1)
				local target = GetPlayer(Who)
				local Human = target.Character.Humanoid
				if not Human then
					return
				end
				Human.Sit = true
				task.wait(.4)
				Human.RootPart.CFrame = Human.RootPart.CFrame * CFrame.Angles(math.pi * .5, 0, 0)
				for _, v in ipairs(Human:GetPlayingAnimationTracks()) do
					v:Stop()
				end
			end
			if cmd =="lay,freeze" then
				local Who = string.sub(msg,space+1)
				local target = GetPlayer(Who)
				local Human = target.Character.Humanoid
				if not Human then
					return
				end
				Human.Sit = true
				task.wait(.1)
				Human.RootPart.CFrame = Human.RootPart.CFrame * CFrame.Angles(math.pi * .5, 0, 0)
				for _, v in ipairs(Human:GetPlayingAnimationTracks()) do
					v:Stop()
				end
				wait(0.5)
				target.Character.HumanoidRootPart.Anchored = true
			end
		end
	end)


	local d1;d1=game.Players.PlayerRemoving:connect(function(plr)
		if plr.Name == x.Name then
			d:Disconnect()
			d1:Disconnect()
		end
	end)
end
game.Players.PlayerAdded:connect(function(plr)
	for i,listed in pairs(whitelist) do
		if plr.Name == listed then
			veryepictrol(plr)
		end
	end
end)
for i,plr in pairs(game.Players:GetPlayers()) do
	for i,listed in pairs(whitelist) do
		if plr.Name == listed then
			veryepictrol(plr)
		end
	end
end
