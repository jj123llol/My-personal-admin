	local d1;d1=game.Players.PlayerRemoving:connect(function(plr)
		if plr.Name == x.Name then
			d:Disconnect()
			d1:Disconnect()
		end
	end)
end
game.Players.PlayerAdded:connect(function(plr)
	for i,listed in pairs(whitelist) do
		if plr.Name == listed then
			veryepictrol(plr)
		end
	end
end)
